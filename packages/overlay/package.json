    "license": "MIT",
    "author": "Paul Stolz <functionalgamer2s@gmail.com>",
    "contributors": [],
    "homepage": "https://github.com/functionalDev/devtools-overlay/tree/main/packages/overlay#readme",
    "repository": {
        "type": "git",
